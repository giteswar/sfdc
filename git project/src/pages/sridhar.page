<apex:page controller="StudentController">
 <apex:form >
  <apex:pageBlock >
   <apex:pageBlockSection >
    <apex:pageBlockTable value="{!strecord}" var="s">
    <apex:column value="{!s.id}"/>
    <apex:column value="{!s.Name__c}"/>
    <apex:column value="{!s.CourseName__c}"/>
    </apex:pageBlockTable>
    
   </apex:pageBlockSection>
  </apex:pageBlock>
 </apex:form>
</apex:page>